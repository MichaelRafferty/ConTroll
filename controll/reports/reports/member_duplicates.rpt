[report]
	name="DuplicateMemberships"
	auth=registration
	csvfile="DuplicateMemberships"
	totals=bottom
[F001]
    name=perid
    sql="p.id"
    title="Perid"
    filter=true
    sort=true
    width=120
    calc=unique
    align=right
[F002]
	name=fullName
	sql="TRIM(REGEXP_REPLACE(CONCAT(p.first_name,' ', p.middle_name, ' ', p.last_name, ' ', p.suffix), ' +', ' '))"
	title="Full Name"
	filter=fullname
	sort=true
	minWidth=300
[F003]
	name=regid
	sql=r1.id
	title="Reg Id"
	align=right
	width=120
[F004]
	name=label1
	sql=m1.label
	title="Label"
	sort=true
	filter=true
	width=225
	format=textarea
[F005]
	name=status
	sql=r1.status
	title="Status"
	filter=list
[F006]
	name=price
	sql=r1.price
	title="Price"
	filter=number
	align=right
[F007]
	name=paid
	sql=r1.paid
	title="Paid"
	filter=number
	align=right
[F013]
	name=dupregid
	sql=r2.id
	title="Dup Reg Id"
	align=right
	width=120
[F014]
	name=duplabel
	sql=m2.label
	title="Dup Label"
	sort=true
	filter=true
    format=textarea
	width=225
[F015]
	name=dupstatus
	sql=r2.status
	title="Dup Status"
	filter=list
[F016]
	name=dupprice
	sql=r2.price
	title="Dup Price"
	filter=number
	align=right
[F017]
	name=dupaid
	sql=r2.paid
	title="Dup Paid"
	filter=number
	align=right
[F100]
    name=first_name
    sql=p.first_name
    visible=false
[F101]
    name=last_name
    sql=p.last_name
    visible=false
[F102]
    name=middle_name
    sql=p.middle_name
    visible=false
[T001]
	name=reg
	alias=r1
[T002]
	name=perinfo
	alias=p
	join="p.id = r1.perid"
[T005]
	name=memLabel
	alias=m1
	join="m1.id = r1.memId"
[T006]
	name=memCategories
	alias=c1
	join="m1.memCategory = c1.memCategory AND c1.onlyOne='Y'"
[T011]
	name=reg
	alias=r2
	join="r1.conid = r2.conid AND r2.perid = r1.perid AND r2.id > r1.id"
[T015]
	name=memLabel
	alias=m2
	join="m2.id = r2.memId"
[T016]
	name=memCategories
	alias=c2
	join="m2.memCategory = c2.memCategory AND c2.onlyOne='Y'"
[P001]
	type=config
	section=con
	item=id
	datatype=i
[where]
	001="r1.conid = ? AND r1.status IN ('unpaid', 'plan', 'paid') AND r2.status IN ('unpaid', 'plan', 'paid')"
	002="AND NOT (m1.memCategory != 'upgrade' AND m2.memCategory = 'upgrade')"
[sort]
	001=p.last_name
	002=p.first_name
	003=p.id
