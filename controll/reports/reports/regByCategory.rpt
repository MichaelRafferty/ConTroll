[report]
	name="Memberships by Category"
	auth=registration
	csvfile="MembershipByCategory"
    index=memCategory
[F001]
	name=Category
	sql=memCategory
	title="Category"
	filter=true
	sort=true
[F002]
	name=Num
	sql="COUNT(*)"
    align=right
	title="Num Memberships"
[T001]
	name=reg
	alias=r
[T002]
	name=memList
	alias=m
	join="m.id = r.memId"
[P001]
	type=config
	section=con
	item=id
	datatype=i
[where]
	001="r.conid = ?"
[group]
    001=memCategory
[sort]
	001=memCategory
